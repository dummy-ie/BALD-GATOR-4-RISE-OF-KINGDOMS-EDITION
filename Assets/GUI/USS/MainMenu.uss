    margin-top: 2%;
}

#ROFLogo {
    --unity-image: url('project://database/Assets/Sprites/ROFLogo.png?fileID=21300000&guid=55d95927156757449ace0a704e0274be&type=3#ROFLogo');
    height: auto;
    align-items: center;
    align-self: center;
    top: -15%;
}

#EnhancedEditionText {
    --unity-image: url('project://database/Assets/Sprites/EnhancedEdition.png?fileID=21300000&guid=e92f4de46426c4f49a4e4a0bcd6da5a3&type=3#EnhancedEdition');
    top: 30%;
    width: auto;
    margin-top: 0;
}

#PlayButton {
    top: 10%;
}
